  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CommonActionForum/liqen-scraper/issues"
  },
  "homepage": "https://github.com/CommonActionForum/liqen-scraper#readme",
  "dependencies": {
    "cheerio": "^0.22.0",
    "date-fns": "^1.27.2",
